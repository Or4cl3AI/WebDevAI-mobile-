{
  "version": 2,
  "name": "webdevai-mobile",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "backend/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/src/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "backend/$1"
    }
  ],
  "env": {
    "STRIPE_SECRET_KEY": "@stripe_secret_key",
    "OPENAI_API_KEY": "@openai_api_key"
  }
}